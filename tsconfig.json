{
  "compilerOptions": { //bu fayldagi kodlarni productionga chiqarayotganda ishlatamiz
  "target": "ES2020", //esma scriptda yozilgan
  "module": "commonjs", //commonjsga compile qiberadi
  "outDir": "./dist", //shu folderga chiqarib beradi
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "baseUrl": "./",
  "paths": {
  "*": ["node_modules/*"]}
},
"include": ["src/**/*.ts"],
"exclude": ["node_modules"]
}